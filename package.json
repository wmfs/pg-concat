{
  "name": "@wmfs/pg-concat",
  "version": "0.0.0-semantically-released",
  "description": "Takes an object with parts from table and outputs sql statement.",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/pg-concat#readme",
  "keywords": [
    "tymly",
    "package",
    "pg",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/pg-concat.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/pg-concat/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "async": "2.6.1",
    "boom": "7.2.0",
    "debug": "3.1.0",
    "lodash": "4.17.10"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.3",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "15.7.2",
    "standard": "11.0.1",
    "@semantic-release/changelog": "2.1.2",
    "@semantic-release/git": "6.0.2"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "build", "scope":"deps", "release": "minor"}
      ]
    }
  }
}
