{
  "name": "@wmfs/pg-concat",
  "version": "0.0.0-semantically-released",
  "description": "Takes an object with parts from table and outputs sql statement.",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/pg-concat#readme",
  "keywords": [
    "tymly",
    "package",
    "pg",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/pg-concat.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/pg-concat/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "lodash": "4.17.18"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "codecov": "3.7.0",
    "conventional-changelog-metahub": "4.0.1",
    "cz-conventional-changelog": "3.2.0",
    "mocha": "8.0.1",
    "nyc": "15.1.0",
    "semantic-release": "17.1.1",
    "standard": "14.3.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "build",
          "scope": "deps",
          "release": "minor"
        }
      ]
    }
  }
}
